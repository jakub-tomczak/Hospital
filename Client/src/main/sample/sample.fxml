<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints maxHeight="258.0" minHeight="10.0" prefHeight="258.0" />
      <RowConstraints maxHeight="288.0" minHeight="66.0" prefHeight="66.0" />
   </rowConstraints>
   <children>
      <Button fx:id="populateData" mnemonicParsing="false" onAction="#buttonMouseClicked" text="Button" GridPane.rowIndex="1" />
      <TableView fx:id="table" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn prefWidth="75.0" text="Name" fx:id="C1" />
          <TableColumn prefWidth="75.0" text="Number" fx:id="C2" />
        </columns>
      </TableView>
   </children>
</GridPane>
